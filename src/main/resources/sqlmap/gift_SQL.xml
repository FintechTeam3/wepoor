<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.hk.wepoor.model.GiftMapper">
	
	<insert id="insert" parameterType="GiftVO">
    <![CDATA[
    INSERT INTO gift (giftId, user_no, giftName, giftPrice) 
    VALUES (NULL, #{user_no}, #{giftName}, #{giftPrice})
    ]]>
	</insert> 
  
  </mapper>